{
    "manifest_version": 2,
  
    "name": "Timestamp Machine",
    "description": "CTRL+F, but for videos.",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "images/icon.png",
     "default_popup": "popup.html"
    },

    "commands": {
        "_execute_browser_action": {
          "suggested_key": {
            "windows": "Ctrl+Shift+F",
            "mac": "Command+Shift+F",
            "linux": "Ctrl+Shift+F"
          }
        }
    },

    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },

    "permissions": [
     "activeTab",
     "storage",
     "tabs"
     ],
     "content_scripts": [
      {
        "matches": [
          "*://*/*"
        ],
        "js": [
          "custom.js"
        ],
        "run_at": "document_end",
        "all_frames": true
      }
    ],
     "content_security_policy":"script-src 'self' https://youtube.com/* https://www.gstatic.com/ https://*.google.com/ https://*.firebaseio.com https://www.googleapis.com 'unsafe-eval'; object-src 'self'"
  }